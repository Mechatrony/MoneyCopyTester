<Page
  x:Class="Backtester.Views.Pages.CoinPage"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:views="using:Backtester.Views"
  xmlns:vmPages="using:Backtester.ViewModels.Pages"
  xmlns:vm="using:Backtester.ViewModels"
  mc:Ignorable="d"
  d:DataContext="{d:DesignInstance vmPages:CoinPageViewModel}"
  Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

  <Grid>
    <TextBlock Text="{Binding Title}"/>
  </Grid>
</Page>
